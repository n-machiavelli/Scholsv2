	<config>
		<!-- ouc:properties -->
		
		<title><!--%echo var="pageTitle" --></title>
		
		<parameter name="page-layout" type="select" group="everyone" prompt="Page Layout" alt="Pick a page layout." section="Page Layouts">
			<!--%echo var="init-page-layout" encoding="none" -->
		</parameter> 
		
		<parameter name="intro-layout" type="select" group="everyone" prompt="Intro Section Layout" alt="Pick a intro section layout.">
			<!--%echo var="init-intro-layout" encoding="none" -->
		</parameter>
		
		<parameter name="giving" type="checkbox" group="Everyone" prompt="Display Giving button" alt="" section="Page options">
			<!--%echo var="init-giving" encoding="none" -->
		</parameter>
		
		<parameter name="sub-mast" type="checkbox" group="Everyone" prompt="Display Sub Mast Section" alt="Should extended sub-mast section be displayed on this page?">
			<!--%echo var="init-sub-mast" encoding="none" -->
		</parameter>
		
		<parameter name="page-tools" type="checkbox" group="page-tools_locked" prompt="Page tools" alt="Should page tools (ShareThis, Social, Print) be displayed on this page?">
			<!--%echo var="init-page-tools" encoding="none" -->
		</parameter>
		
		<parameter name="feedback" type="checkbox" group="feedback_locked" prompt="Feedback Button" alt="Should feedback button be displayed on this page?">
			<!--%echo var="init-feedback" encoding="none" -->
		</parameter>
		
		<!-- Option Box 1 -->
		<parameter name="optbox1-show" type="checkbox" group="Everyone" prompt="Option Box 1" alt="Option Box 1" section="Only used with Option Box layout">
			<option value="show_ob1" selected="false">Yes</option>
		</parameter>	
				
		<parameter name="optbox1-style" type="select" group="optbox1-style_locked" prompt="Box 1 Style" alt="Show on the page? Pick a style.">
			<option value="none_ob1" selected="true">None</option>
			<option value="style1_ob1" selected="false">Style 1</option>
			<option value="style2_ob1" selected="false">Style 2</option>
			<option value="style3_ob1" selected="false">Style 3</option>
			<option value="style4_ob1" selected="false">Style 4</option>
		</parameter> 

		<!-- Option Box 2 -->
		<parameter name="optbox2-show" type="checkbox" group="Everyone" prompt="Option Box 2" alt="Option Box 2">
			<option value="show_ob2" selected="false">Yes</option>
		</parameter>
				
		<parameter name="optbox2-style" type="select" group="optbox2-style_locked" prompt="Box 2 Style" alt="Show on the page? Pick a style.">
			<option value="none_ob2" selected="true">None</option>
			<option value="style1_ob2" selected="false">Style 1</option>
			<option value="style2_ob2" selected="false">Style 2</option>
			<option value="style3_ob2" selected="false">Style 3</option>
			<option value="style4_ob2" selected="false">Style 4</option>
		</parameter>

		<!-- Option Box 3 -->
		<parameter name="optbox3-show" type="checkbox" group="Everyone" prompt="Option Box 3" alt="Option Box 3">
			<option value="show_ob3" selected="false">Yes</option>
		</parameter>
		
					
		<parameter name="optbox3-style" type="select" group="optbox3-style_locked" prompt="Box 3 Style" alt="Show on the page? Pick a style.">
			<option value="none_ob3" selected="true">None</option>
			<option value="style1_ob3" selected="false">Style 1</option>
			<option value="style2_ob3" selected="false">Style 2</option>
			<option value="style3_ob3" selected="false">Style 3</option>
			<option value="style4_ob3" selected="false">Style 4</option>
		</parameter>

		<!-- Option Box 4 -->
		<parameter name="optbox4-show" type="checkbox" group="Everyone" prompt="Option Box 4" alt="Option Box 4">
			<option value="show_ob4" selected="false">Yes</option>
		</parameter>
				
		<parameter name="optbox4-style" type="select" group="optbox4-style_locked" prompt="Box 4 Style" alt="Show on the page? Pick a style.">

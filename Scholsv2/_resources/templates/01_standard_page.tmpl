			<option value="style1_ob4" selected="false">Style 1</option>
			<option value="style2_ob4" selected="false">Style 2</option>
			<option value="style3_ob4" selected="false">Style 3</option>
			<option value="style4_ob4" selected="false">Style 4</option>
		</parameter>
		
		<!-- Responsive options -->
		
		<parameter name="show-resp" type="checkbox" group="sub-mast-resp_locked" prompt="Display at all breakpoints" alt="Should these sections be displayed at all breakpoints?" section="Responsive Page options">
			<option value="sub-mast-resp" selected="true">Sub-mast</option>
			<option value="sub-content-resp" selected="true">Sub-content</option>
			<option value="split-intro-resp" selected="true">2nd Split-Intro Section</option>
		</parameter>
		
		<!-- Admin options -->
		
		<parameter name="sub-content" type="checkbox" group="sub-content_locked" prompt="Display Sub Content Section" alt="Should extended sub-content section be displayed on this page?" section="Admin Page options">
			<option value="show_sub-content" selected="false">Yes</option>
		</parameter>
		
		<parameter name="section-nav" type="checkbox" group="section-nav_locked" prompt="Display Left Navigation" alt="Should section navigation be displayed on this page?">
			<option value="show_section-nav" selected="true">Yes</option>
		</parameter>
		
		<parameter name="sub-section-nav" type="checkbox" group="sub-section-nav_locked" prompt="Display Sub-Nav include" alt="Should sub-nav include be displayed on this page?">
			<option value="show_sub-section-nav" selected="false">Yes</option>
		</parameter>
		
		<parameter name="search-box" type="checkbox" group="search-box_locked" prompt="Display Search Box" alt="Should search box be displayed on this page?">
			<option value="show_search-box" selected="true">Yes</option>
		</parameter>
		
